{
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc", "closure"]
  },
  "source": {
    "include": [
      "BackEnd/controllers",
      "BackEnd/models",
      "Bot/models",
      "Bot/handlers",
      "Bot/src",
      "Bot/services"
    ],
    "includePattern": "\\.(jsx|js|ts|tsx)$",
    "excludePattern": "(node_modules/|docs/|__tests__/|test/|spec/|examples/)"
  },
  "plugins": ["plugins/markdown"],
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": true,
    "default": {
      "outputSourceFiles": true,
      "includeDate": false,
      "useLongnameInNav": true,
      "systemName": "AirForce API & Bot",
      "footer": "",
      "copyright": "AirForce Â© 2025",
      "navType": "vertical",
      "theme": "cosmo",
      "linenums": true,
      "collapseSymbols": true,
      "inverseNav": true
    }
  },
"opts": {
  "destination": "./docs/jsdoc/",
  "encoding": "utf8",
  "private": true,
  "recurse": true,
  "template": "node_modules/docdash",
  "package": "./package.json"
},
  "docdash": {
    "static": true,
    "sort": true,
    "search": true,
    "collapse": true,
    "typedefs": true,
    "removeQuotes": "none",
    "menu": {
      "GitHub Repo": {
        "href": "https://github.com/Davidas666/AirForce",
        "target": "_blank",
        "class": "menu-item",
        "id": "github_repo"
      }
    },
    "sectionOrder": [
      "Tutorials",
      "Classes",
      "Modules",
      "Externals",
      "Events",
      "Namespaces",
      "Mixins",
      "Interfaces"
    ]
  },
  "markdown": {
    "idInHeadings": true
  }
}
